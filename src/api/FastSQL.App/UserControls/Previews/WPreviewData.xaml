<Window x:Class="FastSQL.App.UserControls.Previews.WPreviewData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastSQL.App.UserControls.Previews"
        xmlns:datagrid="clr-namespace:FastSQL.App.UserControls.DataGrid"
        mc:Ignorable="d"
        Title="Preview" Height="450" Width="800"
        WindowStartupLocation="CenterOwner">
    <Grid VerticalAlignment="Stretch">
        <datagrid:UCDataGrid DataContext="{Binding DataGridViewModel}" UseFilter="False" UsePaging="False"></datagrid:UCDataGrid>
    </Grid>
</Window>
