<Window x:Class="FastSQL.App.Middlewares.ApplicationSettings.WApplicationSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastSQL.App.Middlewares.ApplicationSettings"
        xmlns:local1="clr-namespace:FastSQL.App.UserControls"
        mc:Ignorable="d"
        Title="Application Settings" Height="500" Width="1200"
        WindowStartupLocation="CenterScreen">
    <Grid DataContext="{Binding}" Name="grdContainer">
        <local1:UCSettingsContent x:Name="SettingContent" Padding="10, 30, 10, 10"/>
    </Grid>
</Window>
